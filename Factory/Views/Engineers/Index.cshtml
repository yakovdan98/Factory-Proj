@{
  Layout = "_Layout";
}

@using Factory.Models
<div class="engineers w-75">
@if (@Model.Count == 0)
{
  <h3>No engineers have been added yet!</h3>
} 
<ul class="list-group">
@foreach (Engineer engineer in Model)
{
  <li class="list-group-item">@Html.ActionLink($"{engineer.Name}", "Details", new { id = engineer.EngineerId })</li>
}
</ul>
</div>